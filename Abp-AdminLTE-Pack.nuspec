<?xml version="1.0"?>
<package>
    <metadata>
        <id>Abp-AdminLTE-Pack</id>
        <version>0.1</version>
        <title>Abp AdminLTE Pack</title>
        <authors>Tairan Wang</authors>
        <owners>Tairan Wang</owners>
        <licenseUrl>https://github.com/tairan/AdminLTE-Pack/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/tairan/AdminLTE-Pack</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>A web framework based on AdminLTE for Abp.</description>
        <copyright>Copyright 2017 - 2018</copyright>
        <tags>Bootstrap AdminLTE Apb</tags>
        <dependencies>
            <group>
                <dependency id="Abp.Web.Resources" version="2.3.0" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="node_modules\bootstrap\dist\**\*.*" target="Content\bootstrap" />
        <file src="node_modules\html5shiv\dist\**\*.*" target="Content\html5shiv" />
        <file src="node_modules\respond.js\dist\**\*.*" target="Content\respond" />
        <file src="node_modules\bootstrap-colorpicker\dist\**\*.*" target="Content\bootstrap-colorpicker" />
        <file src="node_modules\bootstrap-datepicker\dist\**\*.*" target="Content\bootstrap-datepicker" />
        <file src="node_modules\bootstrap-daterangepicker\daterangepicker.css" target="Content\bootstrap-daterangepicker" />
        <file src="node_modules\bootstrap-daterangepicker\daterangepicker.js" target="Content\bootstrap-daterangepicker" />
        <file src="node_modules\jquery\dist\jquery.*" target="Content\jQuery" />
        <file src="AdminLTE\dist\**\*.*" target="Content\AdminLTE" />
        <file src="AdminLTE\plugins\**\*.*" target="Content" />
        <file src="src\Views\Shared\_Layout.cshtml" target="Views\Shared\_AdminLTE.cshtml" />
    </files>
</package>